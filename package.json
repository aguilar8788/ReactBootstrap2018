  "scripts": {
    "dev": "cross-env NODE_ENV=development env-cmd  .env webpack-dev-server --mode development --open --config ./webpackConfig",
    "prod": "webpack --mode production",
    "test": "NODE_ENV=testing  jest --coverage --verbose",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "NODE_ENV=production webpack --config ./webpackConfig/webpack.config.js --progress"
